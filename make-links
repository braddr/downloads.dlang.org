#!/bin/bash

set -x

bucket=downloads.dlang.org
extraargs="--acl public-read --cache-control max-age=604800"

# $1 == version
# $2 == source
function makelink
{
    toplevel=${2%%/*}
    filename=`basename $2`
    aws --output json --profile ddo s3 cp /tmp/zero-length-file s3://$bucket/$toplevel/${1:0:1}.x/$1/$filename $extraargs --website-redirect http://$bucket/$2

#    cd /media/scratch/ddo
#    mkdir -p /media/scratch/ddo/$toplevel/${1:0:1}.x/$1
#    ln -s ../../../$2 /media/scratch/ddo/$toplevel/${1:0:1}.x/$1/$filename
}

rm /tmp/zero-length-file
touch /tmp/zero-length-file

# makelink 2.068.0 releases/2015/dmd_2.068.0-0_amd64.deb
# makelink 2.068.0 releases/2015/dmd_2.068.0-0_amd64.deb.asc
